{
    "packageName": "uin",
    "description": "A tool for installing and updating packages",
    "autoUpdate": true,
    "rebootAfterUpdate": true,
    "version": "1.1.3",
    "installPath": "/",
    "fileInstalls": [
        {"subPath": "bin/", "name": "uin.lua", "type": "file"},
        {"subPath": "Uinstall/", "name": "packageInstaller.lua", "type": "file"},
        {"subPath": "Uinstall/", "name": "uinutils.lua", "type": "file"},
        {"subPath": "Uinstall/", "name": "bootrun.lua", "type": "file"},
        {"name": "autorun.lua", "type": "file"}
    ]
}